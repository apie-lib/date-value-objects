{
  "name": "apie/date-value-objects",
  "description": "Composer package of the apie library: date value objects",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\DateValueObjects\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\DateValueObjects\\": "tests/"
    }
  },
  "extra": {
    "apie-objects": [
      "Apie\\DateValueObjects\\Ranges\\DateTimeRange",
      "Apie\\DateValueObjects\\DateWithTimezone",
      "Apie\\DateValueObjects\\HourAndMinutes",
      "Apie\\DateValueObjects\\LocalDate",
      "Apie\\DateValueObjects\\Time",
      "Apie\\DateValueObjects\\UnixTimestamp"
    ]
  },
  "require": {
    "php": ">=8.3",
    "apie/core": "self.version",
    "devizzent/cebe-php-openapi": "^1.1.2"
  },
  "require-dev": {
    "apie/fixtures": "self.version",
    "phpunit/phpunit": "^11.5.2",
    "phpspec/prophecy-phpunit": "^2.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}